{
    "name": "aql-parser"
  , "version": "0.0.3"
  , "description": "A parser for AQL in node using jison."
  , "author": "Stan Rozenraukh <stan@stanistan.com>"
  , "dependencies": {
        "jison": "0.3.x"
      , "underscore": "1.4.x"
   }
  , "devDependencies": {
        "jasmine-node": "1.0.x"
   }
  , "engine": "node >= 0.8.11"
  , "main": "./src/aql.js"
  , "scripts": {
        "test": "jasmine-node ./test"
      , "gen-parser": "node_modules/.bin/jison src/aql.jison src/aql.jisonlex -o src/parser.js"
      , "build-test": "npm run-script build-verbose && npm test"
      , "build-verbose": "npm run-script gen-parser > verbose"
    }
}
